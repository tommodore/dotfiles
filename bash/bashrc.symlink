# Bash Completion

if [ -f /etc/bash_completion ]; then
  source /etc/bash_completion
fi

# PATH
pathdirs=(
  /usr/local/heroku/bin
  /opt/local/bin
  /opt/local/sbin
  /usr/local/bin
  /usr/local/sbin
  ~/bin
)
for dir in $pathdirs; do
    if [ -d $dir ]; then
        PATH=$dir:$PATH
    fi
done
export PATH
